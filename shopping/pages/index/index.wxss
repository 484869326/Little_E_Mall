page{
  background-color: #F5F5F5;
}
navbar {
  position: absolute;
  z-index: 1;
}
.navBar_swiper {
  height: calc(100vw*800/1226);
}
.category_wrap {
  overflow: hidden;
  padding: 10rpx 40rpx;
  display: flex;
  flex-direction: column;
}
.category_wrap .category_wrap_explain {
  display: flex;
  justify-content: space-between;
  color: #B9B9B9;
  margin: 10rpx 20rpx;
}
.category_wrap .category_wrap_explain view {
  font-size: 25rpx;
}
.category_wrap .category_wrap_all {
  display: flex;
  flex-wrap: wrap;
}
.category_wrap .category_wrap_all .category_wrap_logo {
  width: 20%;
  position: relative;
  margin-bottom: 2%;
  font-weight: 600;
  text-align: center;
  font-size: 20rpx;
}
.category_wrap .category_wrap_all .category_wrap_logo image {
  width: 70%;
}
.moblie_phone_wrap {
  overflow: hidden;
  padding: 10rpx 40rpx;
}
.moblie_phone_wrap .moblie_phone_title {
  font-size: 33rpx;
  font-weight: 800;
  margin-bottom: 20rpx;
}
.moblie_phone_wrap .moblie_phone_content {
  height: calc(50vw*400/300);
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_left {
  float: left;
  width: 50%;
  height: 100%;
  border-right: 15rpx solid #fff;
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_left swiper {
  width: 100%;
  height: 100%;
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_left swiper swiper-item image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_right {
  float: left;
  width: 50%;
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_right image {
  display: block;
  border-bottom: 15rpx solid #fff;
  height: calc(50vw*400/300/3);
  border-radius: 10rpx;
}
.moblie_phone_wrap .moblie_phone_content .moblie_phone_right image:last-child {
  border-bottom: none;
}
.goodList{
  overflow: hidden;
  padding: 0 2%;
}
.good{
  float: left;
  text-align: center;
  width: 49%;
  margin-bottom: 20rpx;
  background-color: white;
  border-radius: 5rpx;
}
.good:nth-child(odd){
  margin-right: 2%;
}
.goodName{
  font-size: 38rpx;
}
.type{
  margin-top: 5rpx;
  color: #7899C2;
}
.price{
  margin-top: 5rpx;
  color: red;
  font-size: 36rpx;
}
.goDetail{
  padding: 20rpx;
  margin: 10rpx 60rpx;
  color: white;
  font-weight: 600;
  font-size: 32rpx;
  background-color: #EA625B;
  border-radius: 10rpx;
}