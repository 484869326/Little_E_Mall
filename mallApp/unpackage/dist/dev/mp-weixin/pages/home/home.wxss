.title {
  font-weight: bold;
  font-size: 36rpx;
  padding-left: 20rpx;
}
page {
  background-color: #f2f2f2;
}
.image {
  display: block;
  width: 100%;
}
.home {
  position: relative;
}
.home .home-search {
  position: fixed;
  left: 0;
  right: 0;
  z-index: 1;
  background-color: var(--392f7986);
  transition: all 0.3s ease-in-out;
}
.home .home-recommend {
  margin: 0 20rpx;
  border-radius: 20rpx;
  background-color: white;
}
.home .qualifications {
  display: flex;
  margin: 20rpx 0;
}
.home .qualifications view {
  width: 33.33%;
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #f3402b;
  font-size: 28rpx;
}
.home .qualifications view:not(:last-child)::after {
  content: "";
  width: 4rpx;
  height: 36rpx;
  position: absolute;
  left: 100%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 9999rpx;
  background-color: #e6d0cf;
}
.home .qualifications view .qualifications-text {
  padding-left: 8rpx;
}