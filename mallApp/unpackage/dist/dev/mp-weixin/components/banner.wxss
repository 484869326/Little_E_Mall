.title {
  font-weight: bold;
  font-size: 36rpx;
  padding-left: 20rpx;
}
page {
  background-color: #f2f2f2;
}
.image {
  display: block;
  width: 100%;
}
.banner .uni-swiper-dots {
  background-color: rgba(0, 0, 0, 0.3) !important;
  border-radius: 9999rpx;
  height: 6rpx;
  overflow: hidden;
}
.banner .uni-swiper-dots .uni-swiper-dot {
  padding: 0 12rpx;
  background-color: transparent !important;
  margin: 0;
}
.banner .uni-swiper-dots .uni-swiper-dot-active {
  background-color: white !important;
  border-radius: 0 !important;
}
.banner .image {
  width: 100%;
}